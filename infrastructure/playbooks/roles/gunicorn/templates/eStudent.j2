[Unit]
Description=gunicorn daemon
Requires=gunicorn.socket
After=network.target

[Service]
User=root
Group=www-data
WorkingDirectory=/webapps/eStudent
ExecStart=/webapps/opt/venv/bin/gunicorn --access-logfile - --workers 3 --bind unix:/run/gunicorn.sock eStudent.wsgi:application
[Install]
WantedBy=multi-user.target
